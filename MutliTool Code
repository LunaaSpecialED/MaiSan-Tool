@echo off
cls
:login
title Mai San Mutli Tool // User: %USERNAME%  //  Time: %TIME%  //  Date: %DATE%
cls
echo [40;35mPlease login!
set /p login=Username: 
 
if %login%==root goto pass
if %login%==mai goto pass
if %login%==Mai goto pass
if %login%==Lunaa goto pass
if not %login%==root goto e
if not %login%==mai goto e
 
:pass
set /p pass=Password: 
 
if %pass%==san goto Main
if %pass%==root goto Main
if not %pass%==root goto login
if not %pass%==san goto login
 
:e
cls
echo [40;35mWrong username!
ping localhost -n 5 >nul
goto exit
 
 
:exit
exit


